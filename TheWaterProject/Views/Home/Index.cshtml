@{
    ViewData["Title"] = "Home Page";
}

@model ProjectListViewModel

<h1>Water Project</h1>

    @foreach (Project record in Model.Projects)
    {
        <partial name="ProjectSummary" model="record"/>
    }


<div page-model="@Model.PaginationInfo" page-action="Index" page-classes-enable="true" page-class="btn" 
page-class-normal="btn-outline-dark" page-class-selected="btn-primary"
page-url-projectType="@Model.CurrentProjectType" class="btn-group pull-right m-1"></div>